pipeline {
  agent any
  stages {
    stage("Hello") {
      steps {
        echo 'Hello World'
      }
    }
    stage("build") {
      steps {
        echo 'Build'
      }
    }
    stage("test") {
      steps {
        echo 'test'
      }
    }
    stage("deploy") {
      steps {
        echo 'deploy'
      }
    }
  }
}
